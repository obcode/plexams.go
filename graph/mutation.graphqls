type Mutation {
  setSemester(input: String!): Semester!
  # Prepare ZPA-Exams
  zpaExamsToPlan(input: [Int!]!): [ZPAExam!]!
  addZpaExamToPlan(anCode: Int!, unknown: Boolean!): Boolean!
  rmZpaExamFromPlan(anCode: Int!, unknown: Boolean!): Boolean!
