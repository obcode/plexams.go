extend type Query {
  rooms: [Room!]!
}

type Room {
  name: String!
  seats: Int!
  handicap: Boolean!
  lab: Boolean!
  placesWithSocket: Boolean!
  needsRequest: Boolean!
  exahm: Boolean!
  seb: Boolean!
}

type SlotWithRooms {
  dayNumber: Int!
  slotNumber: Int!
  normalRooms: [Room!]!
  exahmRooms: [Room!]!
  labRooms: [Room!]!
  ntaRooms: [Room!]!
}

type PlannedRoom {
  room: Room!
  ancode: Int!
  seatsPlanned: Int!
  duration: Int!
  handicap: Boolean!
  handicapRoomAlone: Boolean!
  reserve: Boolean!
  studentCount: Int!
  ntas: [NTA!]
}

